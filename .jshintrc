{
  "esversion": 8,
  "node": true,
  "browser": false,
  "undef": true,
  "unused": "vars",
  "strict": false,
  "globals": {
    "module": true,
    "require": true,
    "console": true,
    "process": true,
    "__dirname": true,
    "__filename": true
  },
  "expr": true,
  "asi": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "immed": true,
  "indent": 2,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonew": true,
  "plusplus": true,
  "quotmark": "single",
  "regexp": true,
  "trailing": true,
  "white": true
